{
    "baseUrl": "/WebServices",
    "endpoints": [
        {
            "category": "Authentication",
            "endpoints": [
                {
                    "method": "GET",
                    "path": "/auth/verify-token",
                    "summary": "Verificar token de un usuario",
                    "authentication": "x-access-token",
                    "responses": {
                        "200": "Token válido",
                        "401": "Token inválido",
                        "500": "Error interno"
                    }
                },
                {
                    "method": "POST",
                    "path": "/auth/login",
                    "summary": "Login de un usuario",
                    "authentication": "none",
                    "requestBody": {
                        "username": "string",
                        "password": "string"
                    },
                    "responses": {
                        "200": "Usuario logueado con éxito (retorna: login, user_role, id_store si es ROLE_CORP)",
                        "400": "Datos de entrada inválidos o inexistentes",
                        "401": "Password inválida",
                        "404": "Usuario no encontrado",
                        "500": "Error interno"
                    }
                },
                {
                    "method": "POST",
                    "path": "/auth/signup",
                    "summary": "Registro de un usuario",
                    "description": "Permite registrar un nuevo usuario, esta accion solo la puede realizar un usuario logeado",
                    "authentication": "x-access-token (Admin)",
                    "requestBody": {
                        "username": "string",
                        "password": "string",
                        "id_user_role": "integer",
                        "email": "string",
                        "nombre_completo": "string",
                        "tipo_documento": "integer",
                        "numero_doc": "string"
                    },
                    "responses": {
                        "201": "Usuario registrado con éxito",
                        "400": "Datos de entrada inválidos o inexistentes, o error de validación",
                        "401": "Acceso denegado",
                        "409": "El username, email o número de documento ya está en uso",
                        "500": "Error interno del servidor"
                    }
                },
                {
                    "method": "POST",
                    "path": "/auth/forgot-password",
                    "summary": "Solicitar recuperación de contraseña",
                    "description": "Recibe el username del usuario, genera un token de reseteo de contraseña y envía las instrucciones al correo registrado",
                    "authentication": "none",
                    "requestBody": {
                        "username": "string"
                    },
                    "responses": {
                        "200": "Solicitud procesada correctamente",
                        "400": "Datos de entrada inválidos o inexistentes",
                        "404": "Usuario no encontrado",
                        "500": "Error interno"
                    }
                },
                {
                    "method": "POST",
                    "path": "/auth/reset-password",
                    "summary": "Resetear contraseña",
                    "description": "Utiliza el token enviado por email junto con la nueva contraseña. Devuelve un nuevo token JWT listo para iniciar sesión",
                    "authentication": "none",
                    "requestBody": {
                        "usernameOrEmail": "string (opcional)",
                        "username": "string (opcional)",
                        "email": "string (opcional)",
                        "token": "string",
                        "new_password": "string"
                    },
                    "responses": {
                        "200": "Contraseña restablecida exitosamente",
                        "400": "Datos de entrada inválidos o inexistentes",
                        "401": "Token inválido o expirado",
                        "404": "Usuario o rol no encontrado",
                        "500": "Error interno"
                    }
                }
            ]
        },
